      \field{journaltitle}{Physiological Zoology}
      \field{number}{2}
      \field{shorttitle}{Daily {Energy} {Budgets} of {Avian} {Embryos}}
      \field{title}{Daily {Energy} {Budgets} of {Avian} {Embryos}: {The} {Paradox} of the {Plateau} {Phase} in {Egg} {Metabolic} {Rate}}
